{
    "manifest_version": 3,
    "name": "AGIPOCKET",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "128": "src/res/logo.png"
        }
    },
    "icons": {
        "128": "src/res/logo.png"
    },
    "permissions": [
        "storage",
        "alarms"
    ],
    "background": {
        "service_worker": "src/background/index.js",
        "type": "module"
    }
}
